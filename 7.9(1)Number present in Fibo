//the number is in fibonacci series print "Yes
#include<stdio.h>
void main() 
{
   int a=0,b=1,c=0,n;
   scanf("%d",&n);
   if(a==n||b==n)
   printf("yes");
   c=a+b;
   while(c<=n)
   {
    if(c==n)
    {
    printf("yes");break;
    }
    a=b;
    b=c;
    c=a+b;
   }
   if(c!=n)
   printf("No");
}
