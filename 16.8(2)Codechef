#include <stdio.h>
int main()
{
    int a[4],i,p=0;
    for(i=0;i<4;i++)
    {
        scanf("%d",&a[i]);
        
    }
    for(i=0;i<4;i++)
    {
        if(a[i]>=10)
        {
            p++;
            
        }
        else
        {
            continue;
        }
    }
    printf("%d",p);

}
