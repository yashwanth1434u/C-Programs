//String Palindrome without String Functions and String 2
#include <stdio.h>
#include<string.h>
int main()
{
    char s[100];
    int i,length,j=0,count=0;
    gets(s);
    for(i=0;s[i]!='\0';i++);
    length=i;
    j=i-1;
      i=0;
    while(i<length/2)
    {
       if(s[i]!=s[j])
        {
         count++;
         break;
        }
       else
        i++;
        j--;
    }
     if(count==0)
      printf("Palindrome");
      else
      printf("Not palindrome");
      return 0;
}
