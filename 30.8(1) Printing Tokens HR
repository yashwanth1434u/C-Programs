#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
 char str[1000];
 int i;
 gets(str,1000,stdin);
 for(i=0;i<strlen(str);i++)
 {
     if(str[i]==' ')
     {
         printf("\n");
     }
     else
     {
         printf("%c",str[i]);
     }
 }
   
    return 0;
}
