#include<stdio.h>
int main() 
{
    int i,j,n;
    printf("Enter n value:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            printf("%d\t",i*j);
        }
        printf("\n");
    }
    return 0;
}

/*
output:
Enter n value:5
1	2	3	4	5	
2	4	6	8	10	
3	6	9	12	15	
4	8	12	16	20	
5	10	15	20	25	
*/ 
