// Read  2 string and print both are ANAGRAM or NOT ANAGRAM
#include <stdio.h>
#include<string.h>
int main() 
{
    char s1[100],s2[100],n1,n2,count=0;
    int i,j,temp;
    printf("Enter two strings:\n");  
    gets(s1);
    gets(s2);
    n1=strlen(s1);
    n2=strlen(s2);
    if(n1!=n2)
    printf("Not Anagrams");
    else{
    for(i=0;i<n1-1;i++)
    {
        for(j=0;j<n1-i-1;j++)
        {
            if(s1[j]>s1[j+1])
            {
                temp=s1[j];
                s1[j]=s1[j+1];
                s1[j+1]=temp;
            }
            if(s2[j]>s2[j+1])
            {
                temp=s2[j];
                s2[j]=s2[j+1];
                s2[j+1]=temp;
            }
        }
    }
    for(i=0;s1[i]!='\0';i++)
    {
        if(s1[i]==s2[i])
            count++;
        }
        if(count==n1)
        printf("Anagrams");
        else
        printf("Not Anagrams");
    }
     return 0;
}
